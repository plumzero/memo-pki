/*
 * ASN.1 DER decoding routines
 */
 /**
	tag类型为 context-specific 时，tag-length-value的编码格式如下：
		+--------+--------+--------+--...--+--------+--------+--------+----
		|  tag   | length |   len  |       |   len  |  value | value  | val
		| 八位组 | 八位组 | 八位组 |       | 八位组 | 八位组 | 八位组 | 八位
		+--------+--------+--------+--...--+--------+--------+--------+----

		length八位组记录紧随其后的len八位组的个数，也即 len八位组出现的次数根据 length八位组的指示而定，但最多不超过4个；
		value长度（注意，不是lenght八位组存储的大小）采用大端模式存储	
  */
int mbedtls_asn1_get_len( unsigned char **p,
                  const unsigned char *end,
                  size_t *len )
{
    if( ( end - *p ) < 1 )
        return( MBEDTLS_ERR_ASN1_OUT_OF_DATA );

    if( ( **p & 0x80 ) == 0 )			//标签类型为 UNIVERSAL
        *len = *(*p)++;					//先计算 *p，将*p拷贝到另外一个寄存器eax，对该寄存器中的值再*eax，赋值给*len，将原来的(*p)自增一位
    else	//标签类型为 CONTEXT-SPECIFIC，这个应该用不到
    {
        switch( **p & 0x7F )
        {
        case 1:
            if( ( end - *p ) < 2 )
                return( MBEDTLS_ERR_ASN1_OUT_OF_DATA );

            *len = (*p)[1];
            (*p) += 2;
            break;

        case 2:
            if( ( end - *p ) < 3 )
                return( MBEDTLS_ERR_ASN1_OUT_OF_DATA );

            *len = ( (size_t)(*p)[1] << 8 ) | (*p)[2];
            (*p) += 3;
            break;

        case 3:
            if( ( end - *p ) < 4 )
                return( MBEDTLS_ERR_ASN1_OUT_OF_DATA );

            *len = ( (size_t)(*p)[1] << 16 ) |
                   ( (size_t)(*p)[2] << 8  ) | (*p)[3];
            (*p) += 4;
            break;

        case 4:
            if( ( end - *p ) < 5 )
                return( MBEDTLS_ERR_ASN1_OUT_OF_DATA );

            *len = ( (size_t)(*p)[1] << 24 ) | ( (size_t)(*p)[2] << 16 ) |
                   ( (size_t)(*p)[3] << 8  ) |           (*p)[4];
            (*p) += 5;
            break;

        default:
            return( MBEDTLS_ERR_ASN1_INVALID_LENGTH );
        }
    }

    if( *len > (size_t) ( end - *p ) )
        return( MBEDTLS_ERR_ASN1_OUT_OF_DATA );

    return( 0 );
}

int mbedtls_asn1_get_tag( unsigned char **p,		//p指针指向 tag 八位组
                  const unsigned char *end,
                  size_t *len, int tag )	
{
    if( ( end - *p ) < 1 )
        return( MBEDTLS_ERR_ASN1_OUT_OF_DATA );

    if( **p != tag )			//八位组 tag 与传入 tag 校验
        return( MBEDTLS_ERR_ASN1_UNEXPECTED_TAG );

    (*p)++;	//p自增指向 tlv 的 length

    return( mbedtls_asn1_get_len( p, end, len ) );	//返回后，p指向 tlv 的value
}

int mbedtls_asn1_get_bool( unsigned char **p,
                   const unsigned char *end,
                   int *val )
{
    int ret;
    size_t len;

    if( ( ret = mbedtls_asn1_get_tag( p, end, &len, MBEDTLS_ASN1_BOOLEAN ) ) != 0 )
        return( ret );

    if( len != 1 )
        return( MBEDTLS_ERR_ASN1_INVALID_LENGTH );

    *val = ( **p != 0 ) ? 1 : 0;		//只要tlv的value八位组不全为0，即为 true
    (*p)++;		//指向下一个标签，紧凑的编码格式

    return( 0 );
}

int mbedtls_asn1_get_int( unsigned char **p,
                  const unsigned char *end,
                  int *val )
{
    int ret;
    size_t len;

    if( ( ret = mbedtls_asn1_get_tag( p, end, &len, MBEDTLS_ASN1_INTEGER ) ) != 0 )
        return( ret );
	/**
		1.tag-length-value中，无论采用哪种标签类型的编码，value只使用低7bits位进行元素数据的存储，
		  最高的8bit位置0；
		2.如果使用 UNIVERSAL 的标签类型，也即 tag-length-value中，length八位组存储的value的长度，
		  而 CONTEXT-SPECIFIC 的标签类型，其 length八位组存储的是紧随其后真实的value长度编码（大
		  端存储，本页最上面有说明），称为len八位组，len八位组使用 Definite form格式编码，最高的
		  8bit位置1，而只采用低7bits位存储value的长度；
		3.对于 ( **p & 0x80 ) != 0 ，mbedtls 使用了 UNIVERSIAL 的标签类型，因为此时p指向的是length
		  八位组的下一位，所以可以通过p指向八位组的最高8bit位是否为0，来判断是否使用了 UNIVERSAL，
		  如果不为 0，表示编码错误，返回

		  上面一堆的理解都错了, (**p & 0x80) != 0 的意思是：不支持对负值的解析	&（^~^）L
	 */
    if( len == 0 || len > sizeof( int ) || ( **p & 0x80 ) != 0 )	//mbedtls规定，len不应超出int类型的长度
        return( MBEDTLS_ERR_ASN1_INVALID_LENGTH );

    *val = 0;

    while( len-- > 0 )
    {
        *val = ( *val << 8 ) | **p;		//integer使用小端存储，这里对 little-endian 进行还原
        (*p)++;
    }

    return( 0 );
}

#if defined(MBEDTLS_BIGNUM_C)
int mbedtls_asn1_get_mpi( unsigned char **p,
                  const unsigned char *end,
                  mbedtls_mpi *X )
{
    int ret;
    size_t len;

    if( ( ret = mbedtls_asn1_get_tag( p, end, &len, MBEDTLS_ASN1_INTEGER ) ) != 0 )
        return( ret );

    ret = mbedtls_mpi_read_binary( X, *p, len );

    *p += len;

    return( ret );
}
#endif /* MBEDTLS_BIGNUM_C */
/**
	UNIVERSAL 标签类型的 BIT STRING 编码格式
	+--------+--------+-----------+-----------+-----------+----
	|  tag   | length | value for | value for | value for | 
	| octet  | octet  |unused bits| bit string| bit string| ...
	+--------+--------+-----------+-----------+-----------+---
	BIT STRING的 tlv 中的 value 中，每一位存储 unused bits 的数量，确保不超过7
	因为value的第一位要存储 unused bits ，所以 value 至少占用一个八位组

 */
int mbedtls_asn1_get_bitstring( unsigned char **p, const unsigned char *end,
                        mbedtls_asn1_bitstring *bs)
{
    int ret;

    /* Certificate type is a single byte bitstring */
    if( ( ret = mbedtls_asn1_get_tag( p, end, &bs->len, MBEDTLS_ASN1_BIT_STRING ) ) != 0 )
        return( ret );

    /* Check length, subtract one for actual bit string length */
    if( bs->len < 1 )		//最少占一个八位组
        return( MBEDTLS_ERR_ASN1_OUT_OF_DATA );
    bs->len -= 1;

    /* Get number of unused bits, ensure unused bits <= 7 */
    bs->unused_bits = **p;		//p指向value，value的第一位存储unused bits数量
    if( bs->unused_bits > 7 )
        return( MBEDTLS_ERR_ASN1_INVALID_LENGTH );
    (*p)++;	//这自增，指向真正的value

    /* Get actual bitstring */
    bs->p = *p;
    *p += bs->len;

    if( *p != end )
        return( MBEDTLS_ERR_ASN1_LENGTH_MISMATCH );

    return( 0 );
}

/*
 * Get a bit string without unused bits
 */
int mbedtls_asn1_get_bitstring_null( unsigned char **p, const unsigned char *end,
                             size_t *len )
{
    int ret;

    if( ( ret = mbedtls_asn1_get_tag( p, end, len, MBEDTLS_ASN1_BIT_STRING ) ) != 0 )
        return( ret );

    if( (*len)-- < 2 || *(*p)++ != 0 )						//不太理解
        return( MBEDTLS_ERR_ASN1_INVALID_DATA );

    return( 0 );
}



/*
 *  Parses and splits an ASN.1 "SEQUENCE OF <tag>"
 */
 /**
 	以一个ASN.1作为例子
		FooQuestion ::= SEQUENCE{
			trackingNumber		INTEGER(0.199),
			question			IA5String
		}
	PDU
		myQuestion FooQuestion ::= {
			trackNumber		5,
			question		"Anybody there?"
		}
	使用UNIVERSAL标签类型，对上述 SEQUENCE OF 进行编码(hexadecimal)：
		+-----+------+-----+------+-----+-----+------+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+
		| tag |length| tag |length|value|tag  |length|value|value|value|value|value|value|value|value|value|value|value|value|value|value|
		|octet|octet |octet|octet |octet|octet|octet |octet|octet|octet|octet|octet|octet|octet|octet|octet|octet|octet|octet|octet|octet|
		+-----+------+-----+------+-----+-----+------+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+
		|10/20|  13  | 02  |  01  | 05  | 16  |  0e  |  41 |  6e |  79 |  62 |  6f |  64 | 79  |  20 |  74 |  68 | 65  |  72 |  65 |  3f |
		+-----+------+-----+------+-----+-----+------+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+
									5				   'A'	 'n'   'y'   'b'   'o'   'd'   'y'   ' '   't'   'h'   'e'   'r'   'e'   '?'		
  */	
int mbedtls_asn1_get_sequence_of( unsigned char **p,
                          const unsigned char *end,
                          mbedtls_asn1_sequence *cur,
                          int tag)
{
    int ret;
    size_t len;
    mbedtls_asn1_buf *buf;

    /* Get main sequence tag */
    if( ( ret = mbedtls_asn1_get_tag( p, end, &len,
            MBEDTLS_ASN1_CONSTRUCTED | MBEDTLS_ASN1_SEQUENCE ) ) != 0 )		//mbedtls规定，MBEDTLS_ASN1_CONSTRUCTED 与 MBEDTLS_ASN1_SEQUENCE 同类
        return( ret );

    if( *p + len != end )		//总长校验
        return( MBEDTLS_ERR_ASN1_LENGTH_MISMATCH );

    while( *p < end )	//大循环
    {
        buf = &(cur->buf);
        buf->tag = **p;

        if( ( ret = mbedtls_asn1_get_tag( p, end, &buf->len, tag ) ) != 0 )
            return( ret );

        buf->p = *p;
        *p += buf->len;

        /* Allocate and assign next pointer */
        if( *p < end )
        {
            cur->next = (mbedtls_asn1_sequence*)mbedtls_calloc( 1,
                                            sizeof( mbedtls_asn1_sequence ) );

            if( cur->next == NULL )
                return( MBEDTLS_ERR_ASN1_ALLOC_FAILED );

            cur = cur->next;
        }
    }

    /* Set final sequence entry's next pointer to NULL */
    cur->next = NULL;

    if( *p != end )
        return( MBEDTLS_ERR_ASN1_LENGTH_MISMATCH );

    return( 0 );
}
/**
	AlgorithmIdentifier ::= SEQUENCE{
		algorithm		OBJECT IDENTIFIER,
		parameters		ANY DEFINED BY algorithm OPTIONAL
	}
	AlgorithmIdentifier编码示意（与一般的SEQUENCE不同）：
	+------+------+------+------+------+------+------+------+------+------+------+------+------+------+------+------+
	| tag  |length|	tag  |length|value |value |value |value | tag  |length|value |value |value |value |value |value |
	|octet |octet |octet |octet |octet |octet |octet |octet |octet |octet |octet |octet |octet |octet |octet |octet |
	+------+------+------+------+------+------+------+------+------+------+------+------+------+------+------+------+
	
	                            |<-   algorithm value     ->|             |<-           parameters value          ->|
	              |<-                             AlgorithmIdentifier value                                       ->|
 */
int mbedtls_asn1_get_alg( unsigned char **p,
                  const unsigned char *end,
                  mbedtls_asn1_buf *alg, mbedtls_asn1_buf *params )		//alg 保存 algorithm， params 保存 parameters	params可能为空
{
    int ret;
    size_t len;

    if( ( ret = mbedtls_asn1_get_tag( p, end, &len,
            MBEDTLS_ASN1_CONSTRUCTED | MBEDTLS_ASN1_SEQUENCE ) ) != 0 )
        return( ret );

    if( ( end - *p ) < 1 )
        return( MBEDTLS_ERR_ASN1_OUT_OF_DATA );
	
    alg->tag = **p;
    end = *p + len;

    if( ( ret = mbedtls_asn1_get_tag( p, end, &alg->len, MBEDTLS_ASN1_OID ) ) != 0 )
        return( ret );

    alg->p = *p;
    *p += alg->len;

    if( *p == end )	//	parameters为空，这种情况下不能算错
    {
        mbedtls_platform_zeroize( params, sizeof(mbedtls_asn1_buf) );
        return( 0 );
    }

    params->tag = **p;
    (*p)++;

    if( ( ret = mbedtls_asn1_get_len( p, end, &params->len ) ) != 0 )
        return( ret );

    params->p = *p;
    *p += params->len;

    if( *p != end )											//mbedtls规定，一个 AlgorithmIdentifier 中只存在一个 algorithm 和一个 parameters
        return( MBEDTLS_ERR_ASN1_LENGTH_MISMATCH );

    return( 0 );
}

int mbedtls_asn1_get_alg_null( unsigned char **p,		//即 parameters 为空的情况
                       const unsigned char *end,
                       mbedtls_asn1_buf *alg )
{
    int ret;
    mbedtls_asn1_buf params;

    memset( &params, 0, sizeof(mbedtls_asn1_buf) );

    if( ( ret = mbedtls_asn1_get_alg( p, end, alg, &params ) ) != 0 )
        return( ret );

    if( ( params.tag != MBEDTLS_ASN1_NULL && params.tag != 0 ) || params.len != 0 )
        return( MBEDTLS_ERR_ASN1_INVALID_DATA );

    return( 0 );
}

void mbedtls_asn1_free_named_data( mbedtls_asn1_named_data *cur )	//释放 ANY DEFINED BY 中的一个节点，只清空节点内容，不对整个链进行重构
{
    if( cur == NULL )
        return;

    mbedtls_free( cur->oid.p );
    mbedtls_free( cur->val.p );

    mbedtls_platform_zeroize( cur, sizeof( mbedtls_asn1_named_data ) );
}

void mbedtls_asn1_free_named_data_list( mbedtls_asn1_named_data **head )//从前向后，释放 ANY DEFINED BY 的整个链，消灭整个链
{
    mbedtls_asn1_named_data *cur;

    while( ( cur = *head ) != NULL )
    {
        *head = cur->next;
        mbedtls_asn1_free_named_data( cur );
        mbedtls_free( cur );
    }
}
/**
	对于类似于以下的ASN.1类型，根据 contentType，查找其对应的 content
	ContenInfo ::= SEQUENCE{
		contentType		ContentType,
		content		[0]	EXPLICIT ANY DEFINED BY contentType
	}
	ContentType ::= OBJECT IDENTIFIER
 */
mbedtls_asn1_named_data *mbedtls_asn1_find_named_data( mbedtls_asn1_named_data *list,
                                       const char *oid, size_t len )		//oid 是 __In, len 也是 __In
{
    while( list != NULL )
    {
        if( list->oid.len == len &&
            memcmp( list->oid.p, oid, len ) == 0 )
        {
            break;
        }

        list = list->next;
    }

    return( list );
}

